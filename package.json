{
  "name": "mantra",
  "version": "0.5.3",
  "private": true,
  "devDependencies": {
    "chalk": "2.1.0",
    "cj-checklist": "0.6.0",
    "cucumber": "2.3.1",
    "dotenv": "4.0.0",
    "download-file": "0.1.5",
    "exp": "44.0.0",
    "fs-extra": "4.0.2",
    "husky": "0.14.3",
    "inquirer": "3.3.0",
    "jest": "21.2.1",
    "jest-expo": "21.0.2",
    "node-fetch": "1.7.3",
    "react-native-scripts": "0.0.40",
    "react-test-renderer": "16.0.0-alpha.12",
    "semver": "5.4.1"
  },
  "config": {
    "trelloboardjson": "https://trello.com/b/Klg6BFuQ/enspire.json"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "multi \"npm run expo\" \"npm run test:watch\"",
    "expo": "react-native-scripts start",
    "ios": "react-native-scripts ios",
    "git": "babel-node scripts/git.js",
    "test": "npm run test:lint && npm run test:jest && npm run test:features",
    "test:checklist": "checklist",
    "test:watch": "jest --watch --coverage",
    "test:jestupdate": "jest -u",
    "test:jest": "node_modules/.bin/jest --coverage",
    "test:lint": "eslint src",
    "test:features": "cucumber-js",
    "suggestions:get": "babel-node scripts/getSuggestions.js",
    "publish:dev": "npm run deploy:dev && exp publish",
    "publish:live": "npm run git:master && npm run deploy:live && npm i && exp publish",
    "publish:appstore:dev": "npm run publish:dev && babel-node scripts/expoBuild.js",
    "publish:appstore:live": "npm run publish:live && babel-node scripts/expoBuild.js",
    "deploy:dev": "babel-node scripts/deploy.js type=dev",
    "deploy:live": "babel-node scripts/deploy.js type=live",
    "git:master": "git checkout master && git reset --hard && git clean -f -d",
    "build:ios": "exp build:ios",
    "build:status": "exp build:status",
    "login:expo": "exp login",
    "precommit": "babel-node scripts/precommit.js",
    "commitmsg": "babel-node scripts/commitmsg.js"
  },
  "jest": {
    "preset": "jest-expo",
    "collectCoverage": true
  },
  "dependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.2",
    "babel-jest": "21.2.0",
    "babel-plugin-module-resolver": "2.7.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "contentful": "^4.6.2",
    "deep-equal": "1.0.1",
    "enzyme": "3.1.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-import-resolver-babel-module": "3.0.0",
    "eslint-plugin-flowtype": "2.31.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jest": "21.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "expo": "21.0.0",
    "flow-check": "0.2.2",
    "moment": "2.18.1",
    "multi-commands": "1.2.0",
    "prop-types": "15.5.8",
    "react": "16.0.0-alpha.12",
    "react-native": "https://github.com/expo/react-native/archive/sdk-21.0.2.tar.gz",
    "react-native-swiper": "1.5.12",
    "react-redux": "5.0.4",
    "redux": "3.6.0",
    "redux-logger": "3.0.1",
    "redux-persist": "4.8.2",
    "redux-thunk": "2.2.0",
    "sentry-expo": "1.6.0",
    "uuid": "3.1.0"
  }
}
